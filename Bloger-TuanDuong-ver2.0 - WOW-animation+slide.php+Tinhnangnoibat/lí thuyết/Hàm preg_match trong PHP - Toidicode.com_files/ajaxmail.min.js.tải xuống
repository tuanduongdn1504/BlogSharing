function subMail(i){mail=$("#email_feild").val(),""!=mail?validateEmail(mail)?mail.length<6||mail.length>30?$("#es_msg").text("Email không được quá 30 ký tự!"):$.ajax({url:"http://localhost/api/mail/add",type:"POST",data:{mail:mail,csrf_expel_dog:i},success:function(i){200==i?$(".es_msg").text("Thành Công!"):201==i?$(".es_msg").text("Email đã tồn tại trong hệ thống rồi!"):$(".es_msg").text("Có lỗi xảy ra, xin vui lòng thử lại!"),$("#email_feild").val("")},error:function(i){$(".es_msg").text("Có lỗi xảy ra, xin vui lòng thử lại!"),$("#email_feild").val("")}}):$(".es_msg").text("Bạn ơi nhập email cơ mà!"):$(".es_msg").text("Bạn không điền gì sao mình biết")}function validateEmail(i){return/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(i)}