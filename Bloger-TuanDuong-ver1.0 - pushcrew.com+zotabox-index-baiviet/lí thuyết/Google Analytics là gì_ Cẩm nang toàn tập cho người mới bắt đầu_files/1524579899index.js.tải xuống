var current=initial,
step=25,
stepValue=(target - initial) / 25,
interval=setInterval(function(){
current +=stepValue;
step--;
$number.html(prefix + current.toFixed(precision) + suffix);
if(step <=0){
$number.html(prefix + target.toFixed(precision) + suffix);
window.clearInterval(interval);
}}, 40);
});
});
});
