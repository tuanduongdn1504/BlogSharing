errorText=result.data[fieldName];
$input.closest('.w-form-row').addClass('check_wrong')
.find('.w-form-row-state').html(errorText);
}}else{
$resultField.usMod('type', 'error').html(result.data);
}}
},
complete: function(){
$submitBtn.removeClass('loading');
}});
});
});
});
