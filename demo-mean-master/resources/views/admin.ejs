<!--custom-script.js - Add your own theme custom JS-->
<script type="text/javascript" src="/publics/assets/js/custom-script.js"></script>

<!--laod controller-->
<script type="text/javascript" src="/publics/admin/controller/AdminController.js"></script>
<script type="text/javascript" src="/publics/admin/controller/MainController.js"></script>
<script type="text/javascript" src="/publics/admin/controller/PostController.js"></script>

<!-- Toast Notification -->
<script type="text/javascript">
    $(document).ready(function () {
        toastr.options = {
            "closeButton": false,
            "debug": false,
            "newestOnTop": false,
            "progressBar": false,
            "positionClass": "toast-top-right",
            "preventDuplicates": false,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
    })
</script>
