<script type="text/javascript" src="/publics/assets/js/materialize.min.js"></script>

<script type="text/javascript" src="/publics/assets/js/plugins/prism/prism.js"></script>
<!--scrollbar-->
<script type="text/javascript" src="/publics/assets/js/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>

<script src="https://cdn.rawgit.com/alertifyjs/alertify.js/v1.0.10/dist/js/alertify.js"></script>

<!--plugins.js - Some Specific JS codes for Plugin Settings-->
